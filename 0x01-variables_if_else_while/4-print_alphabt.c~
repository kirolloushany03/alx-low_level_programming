#include <stdio.h>
/**
*main -Entry point
*Description: this progrm will print the alphabet without
*q and e
*Return: Always 0 (success)
*/
int main (void)
{
char lc;

for (lc = 'a' ; lc <= 'z' ; lc++)
{
/*dont forget to declare char*/
if (lc != 'q' && lc != 'e')
{
putchar(lc);
}
}
putchar('\n');
return (0);

}
